@model RegistroModel
@{
    ViewData["Title"] = "Confirmar apagar o registro";
}

<div class="text-center">
    <h3>Realmente deseja apgar o Registro (@Model.Name)?</h3>
</div>


<a class="btn btn-primary w-100" asp-action="Index" asp-controller="Registro" role="button">Cancelar exclusão</a>
<br />
<br />
<a class="btn btn-danger w-100" asp-action="Apagar" asp-controller="Registro" asp-route-id="@Model.Id" role="button">Confirmar exclusão</a>
